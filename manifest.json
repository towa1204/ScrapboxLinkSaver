{
  "manifest_version":2,
  "name": "Scrapbox Link Saver",
  "version": "1.0",
  "permissions": ["tabs"],
  "description": "指定したタブのurlとtitleをScrapboxの記法のリンクとしてコピー",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icon16.png"
    },
    "default_title": "Scrapbox Link Saver",
    "default_popup": "popup.html"
  }
}